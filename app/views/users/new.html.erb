<h1>Register a New User</h1>
  <%= form_with url: register_path, method: :post, local: true do |form| %>
    <%= form.label :name, "Name:" %>
    <%= form.text_field :name %><br>
    <%= form.label :email, "Email:" %>
    <%= form.text_field :email %><br>
    <%= form.submit "Create New User" %>
  <% end %>